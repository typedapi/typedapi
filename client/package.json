{
    "name": "typedapi-client",
    "version": "0.3.1",
    "description": "Client library for TypedApi",
    "homepage": "https://typedapi.com",
    "keywords": [
        "typescript",
        "api",
        "server",
        "client"
    ],
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "scripts": {
        "build": "tsc",
        "singleTest": "jest --config jest-config.json -t",
        "test": "eslint src/ && jest --config jest-config.json",
        "testOnly": "jest --config jest-config.json",
        "prepublishOnly": "eslint src/ && jest --config jest-config.json && rm -rf build/ && tsc",
        "lint": "eslint src/"
    },
    "author": {
        "name": "Sergey Sachik",
        "email": "serega3000@gmail.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/typedapi/client.git"
    },
    "peerDependencies": {
        "typedapi-core": "^0.3.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.15",
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2",
        "eslint": "^7.20.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.6",
        "ts-jest": "^26.4.4",
        "typedapi-core": "file:../core",
        "typescript": "^4.1.2"
    },
    "publishConfig": {
        "access": "public"
    }
}
